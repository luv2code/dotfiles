[gui]
[user]
	email = luv2code@gmail.com
	name = Matthew Taylor
[alias]
	sb = status -sb
	tf = tfs fetch
	ct = tfs checkintool
	hard = reset --hard
	a = add .
    co = checkout
	coma = commit -a -m
	com = commit -m
    today = log --stat --since='1 Day Ago' --graph --pretty=oneline --abbrev-commit --date=relative
[credential]
	helper = cache --timeout=3600
[merge]
	tool = meld
   	conflictstyle = diff3
[mergetool "meld"]
	cmd = meld --diff $BASE $LOCAL --diff $BASE $REMOTE --diff $LOCAL $BASE $REMOTE $MERGED
